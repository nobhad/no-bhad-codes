{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/client/landing", "destination": "/client/landing.html" },
    { "source": "/client/portal", "destination": "/client/portal.html" },
    { "source": "/client/intake", "destination": "/client/intake.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
