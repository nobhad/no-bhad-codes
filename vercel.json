{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
      "dest": "/assets/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/fonts/(.*)",
      "dest": "/fonts/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/icons/$1"
    },
    {
      "src": "/client/landing",
      "dest": "/client/landing.html"
    },
    {
      "src": "/client/portal",
      "dest": "/client/portal.html"
    },
    {
      "src": "/client/intake",
      "dest": "/client/intake.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
